pipeline {
	stages {
		stage('Build pipeline_1') {
			steps {
				echo 'Building pipeline_1'
			}
		}
        stage('Test pipeline_1') {
            steps {
			   echo 'Testing pipeline_1'
            }
		}
        stage('Deploy pipeline_1') {
			steps {
                echo 'Deploying pipeline_1'
            }
		}
	}
}
